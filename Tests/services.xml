<?xml version="1.0" ?>
<guzzle>
    <clients>
        <!-- Abstract service to store AWS account credentials -->
        <client name="test.abstract.aws">
            <param name="access_key_id" value="12345" />
            <param name="secret_access_key" value="abcd" />
        </client>
        <client name="test.s3" builder="Guzzle.Service.Aws.S3.S3Builder" extends="test.abstract.aws">
            <param name="devpay_product_token" value="" />
            <param name="devpay_user_token" value="" />
        </client>
        <client name="test.simple_db" builder="Guzzle.Service.Aws.SimpleDb.SimpleDbBuilder" extends="test.abstract.aws" />
        <client name="test.sqs" builder="Guzzle.Service.Aws.Sqs.SqsBuilder" extends="test.abstract.aws" />
        <client name="test.mws" builder="Guzzle.Service.Aws.Mws.MwsBuilder">
            <param name="merchant_id" value="ABCDE" />
            <param name="marketplace_id" value="FGHIJ" />
            <param name="access_key_id" value="AABBCC" />
            <param name="secret_access_key" value="DDEEFF" />
            <param name="application_name" value="GuzzleTest" />
            <param name="application_version" value="0.1" />
            <param name="base_url" value="https://mws.amazonservices.com/" />
        </client>
    </clients>
</guzzle>